{
  "name": "@cubos/env",
  "version": "0.0.0",
  "description": "Strict process.env",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "eslint:fix": "eslint --fix '{src,spec}/**/*.ts'",
    "eslint:check": "eslint '{src,spec}/**/*.ts'",
    "build": "tsc"
  },
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cubos/node-env.git"
  },
  "bugs": {
    "url": "https://github.com/cubos/node-env/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/cubos/node-env#readme",
  "dependencies": {},
  "devDependencies": {
    "@cubos/eslint-config": "^1.0.442740",
    "@types/node": "^15.3.1",
    "jest-extended": "^0.11.5",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/spec/**/*.ts"
    ],
    "verbose": true,
    "testTimeout": 60000,
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  }
}
